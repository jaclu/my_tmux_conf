    "version": "0.2.0",
    "configurations": [
        {
            "type": "bashdb",
            "request": "launch",
            "name": "Bash-Debug tools/myt",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/tools/myt",
            "args": ["-2", "-a", "1.7"]
        },

        {
            "name": "Python Debugger: Build tmux.conf",
            "type": "debugpy",
            "request": "launch",
            "program": "t2.py",
            "console": "integratedTerminal",
            "env": "T2_ENV=1",
            "args": "-r -t /Users/jaclu/.asdf/installs/tmux/1.8/bin/tmux '/Users/jaclu/t2/tmux/tmux.conf'"
        },

        {
            "name": "Python Debugger: rt",
            "type": "debugpy",
            "request": "launch",
            "program": "tools/rt",
            "console": "integratedTerminal",
            "args": ["-2", "-m", "kajsa/jacdroid"]
        }
    ]
}
